<configurationModel>
	<configuration>
	  <expression>plTheory</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>SPLrefinement,set_aux_lemmas,set_comp_lemmas</args>
	  </transformation>
	</configuration>
	
	<configuration>
	  <expression>assets</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>maps, AssetMapping, Assets</args>
	  </transformation>
	</configuration>
	
	<configuration>
	  <expression>populations</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>Population, PopulationTemplates</args>
	  </transformation>
	</configuration>
	
	<configuration>
	  <expression>mpl</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>MultiProductLines</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>intermediateFM</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>FMint, Configuration, FeatureExpression</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>propositionalFM</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>FeatureModel, FeatureModelSemantics, FeatureModelRefinements, FMpInst</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>Or(propositionalFM, propositionalFE)</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>Formula_, FormulaTheory, Name</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>cardinalityFM</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>CardinalityFM</args>
	  </transformation>
	</configuration>
	
	<configuration>
	  <expression>Or(cardinalityFM,cardinalityFE)</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>Name</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>renaming</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>RenamingAssets</args>
	  </transformation>
	</configuration>
	

	<configuration>
	  <expression>intermediateCK</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>CK</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>compositional</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>ConfigurationKnowledge, CKinst, ConfigKnowledgeLaws</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>transformation</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>CKtrans, CKtInst</args>     
	  </transformation>
	</configuration>

	<configuration>
	  <expression>selectAssets</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>CKselect</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>preprocess</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>CKifdef</args>               
	  </transformation>
	</configuration>

	<configuration>
	  <expression>And(selectAssets,preprocess)</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>CKtransMultiple</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>And(intermediateFM,intermediateCK)</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>Specific_FMiCKi</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>And(propositionalFM,intermediateCK)</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>Specific_FMpCKi</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>And(propositionalFM,transformation)</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>Specific_FMpCKt</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>And(propositionalFM,compositional)</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>Specific_FMpCKc</args>
	  </transformation>
	</configuration>

</configurationModel>
