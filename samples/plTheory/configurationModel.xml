<configurationModel>
	<configuration>
	  <expression>plTheory</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>SPLrefinement,set_aux_lemmas,set_comp_lemmas</args>
	  </transformation>
	</configuration>
	
	<configuration>
	  <expression>populations</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>Population</args>
	  </transformation>
	</configuration>
	
	<configuration>
	  <expression>mpl</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>MultiProductLines</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>propositionalFM</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>FeatureModel, FeatureModelSemantics, FeatureModelRefinements</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>Or(propositionalFM, propositionalFE)</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>Formula_, FormulaTheory,Name</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>cardinalityFM</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>CardinalityFM</args>
	  </transformation>
	</configuration>
	
	<configuration>
	  <expression>Or(cardinalityFM,cardinalityFE)</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>Name</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>renaming</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>RenamingAssets</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>compositional</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>ConfigurationKnowledge, CKinst</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>transformation</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>CKtrans, CKtInst</args>     
	  </transformation>
	</configuration>

	<configuration>
	  <expression>selectAssets</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>CKselect, CKselectInst</args>
	  </transformation>
	</configuration>

	<configuration>
	  <expression>preprocess</expression>
	  <transformation>
	    <name>selectComponents</name>
	    <args>CKifdef</args>               
	  </transformation>
	</configuration>

</configurationModel>
