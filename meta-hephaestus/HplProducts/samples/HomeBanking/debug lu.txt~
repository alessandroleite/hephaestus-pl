  Configuration {expression = \"transfer\", transformations = [UseCaseTransformation (SelectScenarios [\"SC02\"])]},
  Configuration {expression = \"PIN\", transformations = [UseCaseTransformation (EvaluateAspects [\"ADV01\"])]},
  Configuration {expression = And (\"biometrics\", \"fingerprint\"), transformations = [UseCaseTransformation (EvaluateAspects [\"ADV02\"])]},
