--
-- Syntax of Hephaestus transformations
-- Factored out to avoid cyclic dependencies
--

module HplAssets.Hephaestus.Transformation where

data HephaestusTransformation
   = SelectEmptyProduct
   | SelectAsset String
   | SelectExport String
   | BindProductNameM1 String
   | BindProductNameM2 String
 deriving (Show)
